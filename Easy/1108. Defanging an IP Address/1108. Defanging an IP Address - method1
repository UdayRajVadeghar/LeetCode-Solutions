class Solution {
    public String defangIPaddr(String address) {

        String s = "";

        for(int i = 0 ; i < address.length() ; i++){
            if(address.charAt(i) == '.'){
                s = s.concat("[.]");
            }else{
                char temp = address.charAt(i);
                String temp1 = Character.toString(temp);
                s = s.concat(temp1);
            }
        }
        return s;

    }
}
